package com.will.os.repositories;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;
import org.springframework.stereotype.Repository;

import com.will.os.domain.Cliente;
import com.will.os.domain.Tecnico;

@Repository
public interface ClienteRepository extends JpaRepository<Tecnico, Integer> {
	
	@Query("SELECT obj FROM Cliente obj WHERE obj.cpf =:cpf")
	Tecnico findByCPF(@Param("cpf") String cpf);

	void saveAll(List<Cliente> asList);
	
}
